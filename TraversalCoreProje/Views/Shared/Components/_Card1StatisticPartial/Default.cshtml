@*
    _Card1StatisticPartial
*@

<div class="col-lg-4 col-md-4 order-1">
    <div class="row">
        <div class="col-lg-6 col-md-12 col-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <div class="card-title d-flex align-items-start justify-content-between">

                        <div class="avatar flex-shrink-0">
                            <span class="avatar-initial rounded bg-label-success">
                                <i class="bx bx-trending-up"></i>
                            </span>
                        </div>
                    </div>
                    <span class="fw-semibold d-block mb-1">Tur Sayısı</span>
                    <h3 class="card-title mb-2">@ViewBag.v1</h3>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-12 col-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <div class="card-title d-flex align-items-start justify-content-between">

                        <div class="avatar flex-shrink-0">
                            <span class="avatar-initial rounded bg-label-info">
                                <i class="bx bx-group"></i>
                            </span>
                        </div>
                    </div>
                    <span class="fw-semibold d-block mb-1">Misafirler</span>
                    <h3 class="card-title text-nowrap mb-1">@ViewBag.v2</h3>
                </div>
            </div>
        </div>
    </div>
</div>